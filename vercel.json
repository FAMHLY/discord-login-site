{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/bot.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/bot/(.*)",
      "dest": "api/bot.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}